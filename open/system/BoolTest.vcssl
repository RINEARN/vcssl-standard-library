encode UTF-8;

import open.system.Bool;

/**
 * <p>
 * open.system.Bool モジュールのテストです。
 * </p>
 */


test();

void test(){
	testIsBool();
	println("open.system.Bool: OK.");
}


/**
 * isBool のテストです。
 *
 * @author 松井文宏
 */
public void testIsBool(){
	assert( isBool("true") );
	assert( isBool("false") );
	assert( !isBool("100") );
	assert( !isBool("abc") );
}

