 * open.system.String モジュールのテストです。
 * </p>
 */


test();

void test(){
	testToStringArray();

	println("open.system.String: OK.");
}



/**
 * toStringArray のテストです。
 *
 * @author 松井文宏
 */
public void testToStringArray(){
	string array[] = toStringArray("A", "B", "C", "D", "E");
	assert(length(array, 0) == 5);
	assert(array[0] == "A");
	assert(array[1] == "B");
	assert(array[2] == "C");
	assert(array[3] == "D");
	assert(array[4] == "E");
}

